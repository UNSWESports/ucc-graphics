{
  "name": "ucc-overwatch",
  "version": "1.0.0",
  "description": "UNSW Contenders Cup Graphics Package for Overwatch 2",
  "homepage": "https://unswesports.com.au",
  "author": {
    "name": "Xander Gonzaga",
    "email": "xandergonzaga@gmail.com",
    "url": "https://riotline.dev"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "*",
    "bundleDependencies": {
      "ucc-main": "^1.0.0"
    },
    "dashboardPanels": [
      {
        "name": "ow-player-list",
        "title": "Overwatch Player List",
        "width": 5,
        "file": "ow-player-list.html",
        "headerColor": "#525F78",
        "workspace": "UCC Overwatch"
      },
      {
        "name": "ow-map-bans",
        "title": "Overwatch Map Bans",
        "width": 5,
        "file": "ow-map-bans.html",
        "headerColor": "#525F78",
        "workspace": "UCC Overwatch"
      }
    ],
    "graphics": [
      {
        "file": "game_overlay/index.html",
        "width": 1920,
        "height": 1080
      },{
        "file": "team_versus/index.html",
        "width": 1920,
        "height": 1080
      },{
        "file": "map_bans/index.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "assetCategories": [
      {
        "name": "maps",
        "title": "Map Ban Images",
        "allowedTypes": ["jpg", "jpeg", "gif", "png"]
      }
    ]
  }
  
}
