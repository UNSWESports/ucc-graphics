{
  "name": "ucc-valorant",
  "version": "1.3.0",
  "description": "UNSW Contenders Cup Graphics Package for Valorant",
  "homepage": "https://unswesports.com.au",
  "author": {
    "name": "Xander Gonzaga",
    "email": "xandergonzaga@hotmail.com",
    "url": "https://riotline.dev"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "*",
    "bundleDependencies": {
      "ucc-main": "^1.0.0"
    },
    "dashboardPanels": [
      {
        "name": "ucc-valorant-scoreboard",
        "title": "VALORANT Scoreboard",
        "width": 3,
        "file": "ucc-valorant.html",
        "headerColor": "#525F78",
        "workspace": "UCC Valorant"
      },
      {
        "name": "ucc-valorant-mapveto",
        "title": "VALORANT Map Veto",
        "width": 4,
        "file": "mapVeto/mapVeto.html",
        "headerColor": "#525F78",
        "workspace": "UCC Valorant"
      },
      {
        "name": "ucc-valorant-agentselect",
        "title": "VALORANT Agent Select",
        "width": 3,
        "file": "agentSelect.html",
        "headerColor": "#525F78",
        "workspace": "UCC Valorant"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "scoreboard/index.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "agentSelect/index.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "mapVeto/bo1Fullscreen/index.html",
        "width": 1920,
        "height": 1080
      },
	  {
        "file": "mapVeto/bo3Fullscreen/index.html",
        "width": 1920,
        "height": 1080
      },
      {
      "file": "roundOutcome/index.html",
      "width": 1920,
      "height": 1080
      }
    ],
    "assetCategories": [
      {
        "name": "mapimages",
        "title": "Map Ban Images",
        "allowedTypes": ["jpg", "jpeg", "gif", "png", "webp"]
      },
      {
        "name": "mapvideos",
        "title": "Map Ban Videos",
        "allowedTypes": ["mp4"]
      },
      {
        "name": "mapvetobuild1",
        "title": "Map Ban Build 1 Audio",
        "allowedTypes": ["mp3"]
      },
      {
        "name": "mapvetobuild2",
        "title": "Map Ban Build 2 Audio",
        "allowedTypes": ["mp3"]
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "Apache-2.0",
  "dependencies": {
  }
}
