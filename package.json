{
  "name": "ucc-main",
  "version": "1.0.0",
  "description": "UNSW Contenders Cup Main Package. Required for base functionality and interconnects.",
  "homepage": "https://unswesports.com.au",
  "author": {
    "name": "Xander Gonzaga",
    "email": "xandergonzaga@gmail.com",
    "url": "https://riotline.dev"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "*",
    "dashboardPanels": [
      {
        "name": "ucc-teams",
        "title": "UCC Teams",
        "width": 4,
        "file": "teams/ucc-teams.html",
        "headerColor": "#525F78",
        "workspace": "UCC Main"
      },
      {
        "name": "ucc-main",
        "title": "UCC Settings",
        "width": 2,
        "file": "main/ucc-main.html",
        "headerColor": "#525F78",
        "workspace": "UCC Main"
      },
      {
        "name": "ucc-startgg",
        "title": "UCC StartGG API",
        "width": 3,
        "file": "startgg/startgg.html",
        "headerColor": "#525F78",
        "workspace": "UCC Main"
      }
    ],
    "graphics": [
      {
        "file": "game_overlay/index.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "versus/versus.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "license": "Apache-2.0",
